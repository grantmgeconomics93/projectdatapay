library(dbplyr)
library(tidyverse)
library(stringi)
library(readxl)
data_cleaned_2021 <- read_excel("data_cleaned_2021.xlsx")
View(data_cleaned_2021)
newmin=substr(data_cleaned_2021$`Salary Estimate`,2,4)
newwmin=gsub("K","",(newmin))%>%as.numeric()
newdata=cbind(newwmin,data_cleaned_2021)
newmax=substr(data_cleaned_2021$`Salary Estimate`,7,9)
newdata$newmax=gsub("K","",(newmax))%>%as.numeric()
